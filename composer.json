{
    "name": "dazzle-app/audience",
    "description": "A Laravel package to accept email signups during prelaunch",
    "license": "MIT",
    "authors": [
        {
            "name": "Andrew Huggins",
            "email": "andrewhuggins@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "DazzleApp\\Audience\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DazzleApp\\Audience\\Providers\\DazzleAppAudienceServiceProvider"
            ]
        }
    },
    "require-dev": {
        "orchestra/testbench": "~3.7",
        "phpunit/phpunit": "^7.5"
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html tests/_reports"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    }
}
